##获取所有学生信息
#list
SELECT
  student.account_id AS id,
  department.dept_id AS dept,
  department.`dept_name`,
  name,
  type,
  account,
  gender,
  _c,
  _d
FROM student
  join account ON student.account_id = account.account_id
  join department ON student.dept_id = department.dept_id

##获取条数
#total
SELECT count(student_id) AS total
FROM student
  JOIN account ON student.account_id = account.account_id
  join department ON student.dept_id = department.dept_id


